<?xml version="1.0" encoding="UTF-8"?>
 
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
           id="org.4visiontech.plugin"
      version="0.1.0">
    <name>Plugin</name>
    <description>Sample PhoneGap Plugin</description>
    <license>MIT</license>
    <keywords>phonegap,plugin,4visiontech</keywords>
 
    <engines>
      <engine name="cordova" version=">=3.0.0" />
    </engines>    
 
    <js-module src="www/plugin.js" name="Plugin">
        <clobbers target="window.calendar" />
    </js-module>
 
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="Plugin">
                <param name="android-package" value="org.apache.cordova.plugin.4visiontech.Plugin"/>
            </feature>
        </config-file>
 
        <source-file src="src/android/Plugin.java" target-dir="src/org/apache/cordova/plugin/4visiontech" />     
     </platform>         
</plugin>